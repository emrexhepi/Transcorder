{
  "name": "transcorder",
  "version": "0.0.1",
  "description": "RaspberryPi based hardware accelerated FFMPEG transcoder and recorder",
  "main": "server/bin/server.js",
  "scripts": {
    "dev": "webpack-dev-server --config webpack/client.dev.js",
    "build": "npm run build:client && npm run build:server",
    "build:server": "cross-env NODE_ENV=production webpack --config webpack/server.prod.js --progress",
    "build:server-dev": "cross-env NODE_ENV=development webpack --config webpack/server.dev.js",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack/client.prod.js --progress",
    "start": "cd server && node bin/server.js",
    "start-inspect": "cd server && node --inspect bin/server.js",
    "server-dev": "nodemon",
    "lint": "eslint --ext .jsx,.js ./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emrexhepi/transcorder"
  },
  "keywords": [
    "react",
    "express",
    "webpack",
    "babel",
    "expressjs",
    "react router v4",
    "redux"
  ],
  "author": "Emrullah Rexhepi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/emrexhepi/transcorder/issues"
  },
  "homepage": "https://github.com/emrexhepi/transcorder#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "cross-env": "^5.2.0",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "lowdb": "^1.0.0",
    "luxon": "^1.3.3",
    "npm": "^6.4.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-hot-loader": "^4.3.5",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "systeminformation": "^3.44.0",
    "tree-kill": "^1.2.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-async-to-generator": "^7.0.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "node-hot-loader": "^1.10.1",
    "node-sass": "^4.9.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^4.17.1",
    "webpack-cli": "^2.1.5",
    "webpack-dev-server": "^3.1.14",
    "webpack-node-externals": "^1.7.2"
  },
  "bundledDependencies": [
    "cross-env",
    "ejs",
    "express",
    "prop-types",
    "react",
    "react-dom",
    "react-redux",
    "react-router",
    "react-router-dom",
    "redux"
  ]
}
